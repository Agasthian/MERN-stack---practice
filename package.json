  "dependencies": {
    "body-parser": "^1.20.1",
    "concurrently": "^5.0.2",
    "cookie-session": "^1.3.3",
    "express": "^4.18.2",
    "mongoose": "^5.7.12",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "stripe": "^8.3.0"
  }
}
