{
  "name": "server",
  "version": "1.0.0",
  "description": "Feed back server app",
  "engines": {
    "node": "12.11.0",
    "npm": "6.11.2"
  },
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
